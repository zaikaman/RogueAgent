{
  "name": "rogue-agent-backend",
  "version": "1.0.0",
  "description": "Backend for Rogue Crypto Alpha Oracle",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "heroku-postbuild": "npm run build",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:agents": "npx ts-node src/agents/__tests__/run-all-tests.ts",
    "test:scanner": "npx ts-node src/agents/__tests__/run-scanner.test.ts",
    "test:analyzer": "npx ts-node src/agents/__tests__/run-analyzer.test.ts",
    "test:api": "npx ts-node src/api/__tests__/run-api-tests.ts",
    "test:integration": "npm run test:api"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.69",
    "@gradio/client": "^2.0.0",
    "@iqai/adk": "^0.5.6",
    "@supabase/supabase-js": "^2.39.7",
    "axios": "^1.13.2",
    "canvas": "^3.2.0",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "express": "^4.18.3",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.6",
    "hyperliquid": "^1.7.7",
    "morgan": "^1.10.0",
    "node-telegram-bot-api": "^0.66.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@types/node-telegram-bot-api": "^0.64.12",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^30.2.0",
    "prettier": "^3.2.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
